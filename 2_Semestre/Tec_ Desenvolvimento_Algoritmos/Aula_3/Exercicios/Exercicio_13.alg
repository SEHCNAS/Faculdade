Inicio
	
	Real TrianguloBase, TrianguloAltura, TrianguloArea;
	
	Escreva("Digite o valor da altura: ");
	Leia(TrianguloAltura);
	
	Escreva("Digite o valor da base: ");
	Leia(TrianguloBase);
	
	TrianguloArea = (TrianguloBase * TrianguloAltura) / 2;
	
	Escreva("A area do triangulo é: " + TrianguloArea);
	
Fim